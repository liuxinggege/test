    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,ts,tsx}": [
            "prettier --single-quote --tab-width=4 --write",
            "git add"
        ]
    },
    "prettier": {
        "singleQuote": true,
        "tabWidth": 4,
        "no-debugger": false
